{
  "expo": {
    "name": "Viva-App-frontend",
    "slug": "Viva-App-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/v-logo-3.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/v-logo-3.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/v-logo-3.png",
        "backgroundColor": "#000000"
      },
      "splash": {
        "image": "./assets/v-logo-3.png",
        "resizeMode": "cover",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
